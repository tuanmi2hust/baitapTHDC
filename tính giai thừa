#include<stdio.h>

int main(){
    int n;
    scanf("%d", &n);
    if(n >= 8 || n< 0){
        printf("ERROR");
        return 0;
    }
    if(n == 0){
        printf("0");
        return 0;
    }
    int giaithua = 1;
    for(int i = 1; i < n + 1; i++){
        giaithua = giaithua * i;
    }
    printf("%d", giaithua);
    
    return 0;
}
